

class  A
{
	public static void main(String[] args) {
		int i = 3;
		int j = 5;
		System.out.println("addition of "  + i +  " and " + j +  " is: " + (i+j));
		System.out.println("substraction of " + i + " and " + j + " is: " + (i-j));
